        "\n",
        "a = arr.array('u', ['d', 'e', 'g', 'u', 'z', 'm', 'a', 'n'])\n",
        "\n",
        "letter_dict = {}\n",
        "for i in range(len(a)):\n",
        "    letter_dict[i + 1] = a[i]\n",
        "\n",
        "print(\"First element is:\", letter_dict[1])\n",
        "print(\"Second element is:\", letter_dict[2])\n",
